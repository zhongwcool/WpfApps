<Window x:Class="App05.Task.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="异步任务与取消" Height="450" Width="800">
    <Grid>
        <RichTextBox x:Name="Info" />
        <WrapPanel HorizontalAlignment="Right" VerticalAlignment="Bottom">
            <Button Click="ButtonRenew_OnClick" Padding="10">新生</Button>
            <Button Click="ButtonStop_OnClick" Padding="10">Dont Click</Button>
        </WrapPanel>
    </Grid>
</Window>